{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "fiz",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "fiz",
      "kebab-case"
    ],
    "no-string-literal": false,
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "no-console": [true, "log", "error"]
  }
}
